@require 'zorium-paper/base'

@require '../../vars'

.z-profile-history
  flex: 1
  overflow: auto
  -webkit-overflow-scrolling: touch
  padding: 16px

  > .g-grid

    > .title
      font-subhead()
      padding: 16px 0
      color: getColor('$primary500')

    > .deck
      margin-bottom: 16px

    > .divider
      width: 100%
      height: 1px
      background: getColor('$white12')
      margin: 8px 0
